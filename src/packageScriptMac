# arg2 is the app name, arg1 is the exe name
if [ $# == 2 ]; then
   mkdir -p ../prod/$2.app/Contents/MacOS
   mv -f $1 ../prod/$2.app/Contents/MacOS/$1
   cp -rf ../Resources ../prod/$2.app/Contents/Resources
   cp -rf ../Frameworks ../prod/$2.app/Contents/Frameworks
else
   printf "Usage: packageScriptMac <executable> <name>"
fi

